{
  "cli": {
    "version": ">= 5.6.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "ios": {
        "buildConfiguration": "Debug"
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "development",
      "distribution": "internal",
      "developmentClient": true
    },
    "preview": {
      "ios": {
        "enterpriseProvisioning": "universal"
      },
      "android": {
        "image": "ubuntu-22.04-jdk-17-ndk-r21e",
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}